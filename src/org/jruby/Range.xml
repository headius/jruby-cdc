<module type="class">
  <name>Range</name>
  <superclass>Object</superclass>
  <includes>Enumerable</includes>
  <implementation>org.jruby.RubyRange</implementation>

  <instance-methods>
    <method name="==">
       <arity>1</arity>
       <java>equal</java>
    </method>
    <method name="===">
       <arity>1</arity>
       <java>op_eqq</java>
    </method>
    <method name="first"/>
    <method-alias name="begin" original="first"/>
    <method name="last"/>
    <method-alias name="end" original="last"/>
    <method name="inspect"/>
    <method name="to_s" original="inspect"/>
    <method name="length"/>
    <method-alias name="size" original="length"/>
    <method name="exclude_end?">
       <java>exclude_end_p</java>
    </method>
    <method name="each"/>
    <method name="initialize">
       <arity optional="true">2</arity>
    </method>
  </instance-methods>

  <class-methods/>
</module>