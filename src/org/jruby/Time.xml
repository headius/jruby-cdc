<?xml version="1.0"?>

<module type="class">
  <name>Time</name>
  <superclass>Object</superclass>
  <includes>Comparable</includes>
  <implementation>org.jruby.RubyTime</implementation>

  <instance-methods>
    <method name="&lt;=>">
      <arity>1</arity>
      <java>op_cmp</java>
    </method>  
    <method name="-">
      <arity>1</arity>
      <java>op_minus</java>
    </method>
    <method name="+">
      <arity>1</arity>
      <java>op_plus</java>
    </method>
    <method name="sec"/>
    <method name="min"/>
    <method name="hour"/>
    <method name="mday"/>
    <method-alias name="day" original="mday"/>
    <method name="month"/>
    <method-alias name="mon" original="month"/>
    <method name="year"/>
    <method name="wday"/>
    <method name="yday"/>
    <method name="isdst"/>
    <method name="zone"/>
    <method name="to_a"/>
    <method name="to_f"/>
    <method name="asctime"/>
    <method-alias name="ctime" original="asctime"/>
    <method name="to_s"/>
    <method-alias name="inspect" original="to_s"/>
    <method name="strftime">
      <arity>1</arity>
    </method>
    <method name="usec"/>
    <method-alias name="tv_usec" original="usec"/>
    <method name="to_i"/>
    <method-alias name="tv_sec" original="to_i"/>
    <method name="gmtime"/>
    <method-alias name="utc" original="gmtime"/>
    <method name="gmt?">
      <java>gmt</java>
    </method>
    <method-alias name="gmtime?" original="gmt?"/>
    <method-alias name="utc?" original="gmt?"/>
    <method name="localtime"/>
    <method name="hash"/>
  </instance-methods>

  <class-methods>
    <method name="at">
      <arity optional="true">0</arity>
      <java>new_at</java>
    </method>
    <method name="new">
      <java>s_new</java>
    </method>
    <method-alias name="now" original="new"/>
    <method name="local">
      <arity optional="true">0</arity>
      <java>new_local</java>
    </method>
    <method-alias name="mktime" original="local"/>
    <method name="utc">
      <arity optional="true">0</arity>
      <java>new_utc</java>
    </method>
    <method-alias name="gm" original="utc"/>
  </class-methods>
</module>
