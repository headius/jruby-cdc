<module type="class">
  <name>Float</name>
  <superclass>Numeric</superclass>
  <implementation>org.jruby.RubyFloat</implementation>

  <instance-methods>
    <method name="ceil"/>
    <method name="finite?">
      <java>finite_p</java>
    </method>
    <method name="floor"/>
    <method name="infinite?">
      <java>infinite_p</java>
    </method>
    <method name="nan?">
      <java>nan_p</java>
    </method>
    <method name="round"/>
    <method name="truncate"/>
    <method name="to_f"/>
    <method name="to_i"/>
    <method name="to_s"/>
    <method name="zero?">
      <java>zero_p</java>
    </method>
    <method name="&lt;">
      <arity>1</arity>
      <java>op_lt</java>
    </method>
    <method name="&lt;=">
      <arity>1</arity>
      <java>op_le</java>
    </method>
    <method name="&lt;=>">
      <arity>1</arity>
      <java>op_cmp</java>
    </method>
    <method name=">">
      <arity>1</arity>
      <java>op_gt</java>
    </method>
    <method name=">=">
      <arity>1</arity>
      <java>op_ge</java>
    </method>
    <method name="%">
      <arity>1</arity>
      <java>op_mod</java>
    </method>
    <method name="/">
      <arity>1</arity>
      <java>op_div</java>
    </method>
    <method name="+">
      <arity>1</arity>
      <java>op_plus</java>
    </method>
    <method name="-">
      <arity>1</arity>
      <java>op_minus</java>
    </method>
    <method name="*">
      <arity>1</arity>
      <java>op_mul</java>
    </method>
    <method name="**">
      <arity>1</arity>
      <java>op_pow</java>
    </method>
    <method name="eql?">
      <arity>1</arity>
      <java>op_eql</java>
    </method>
    <method name="==">
      <arity>1</arity>
      <java>op_equal</java>
    </method>
    <method-alias name="===" original="=="/>
    <method name="hash"/>
  </instance-methods>

  <class-methods>
    <method name="induced_from">
       <arity>1</arity>
    </method>
    <undefine-method name="new"/>
  </class-methods>
</module>