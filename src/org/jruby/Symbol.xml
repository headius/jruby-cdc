<?xml version="1.0"?>

<module type="class">
  <name>Symbol</name>
  <superclass>Object</superclass>
  <implementation>org.jruby.RubySymbol</implementation>

  <instance-methods>
    <method name="to_i"/>
    <method-alias name="to_int" original="to_i"/>
    <method name="to_s"/>
    <method-alias name="id2name" original="to_s"/>
    <method name="==">
      <arity>1</arity>
      <java>equal</java>
    </method>
    <method name="hash"/>
    <method name="inspect"/>
    <method name="clone">
      <java>rbClone</java>
    </method>
    <method-alias name="dup" original="clone"/>
    <method name="freeze"/>
    <method name="taint"/>
  </instance-methods>

  <class-methods>
    <undefine-method name="new"/>
  </class-methods>
</module>
