<module type="class">
  <name>Integer</name>
  <superclass>Numeric</superclass>
  <includes>Precision</includes>
  <index-offset>0x6100</index-offset>

  <implementation>org.jruby.RubyInteger</implementation>

  <instance-methods>
    <method name="chr"/>
    <method name="downto">
      <arity>1</arity>
    </method>
    <method name="integer?">
      <java>int_p</java>
    </method>
    <method name="next"/>
    <method name="step">
      <arity>2</arity>
    </method>
    <method-alias name="succ" original="next"/>
    <method name="times"/>
    <method name="upto">
      <arity>1</arity>
    </method>
  </instance-methods>

  <class-methods>
    <method name="induced_from">
      <arity>1</arity>
    </method>
    <undefine-method name="new"/>
  </class-methods>
</module>
