<module type="class">
  <name>Proc</name>
  <superclass>Object</superclass>
  <implementation>org.jruby.RubyProc</implementation>

  <!-- call and [] may be wrong (more specific arity) -->
  <instance-methods>
    <method name="call">
       <arity optional="true">0</arity>
    </method>
    <method-alias name="[]" original="call"/>
    <method-alias name="yield" original="call"/>
    <method name="arity"/>
  </instance-methods>

  <class-methods>
    <method name="new">
      <arity optional="true">0</arity>
      <java>newInstance</java>
    </method>
  </class-methods>
</module>