<module type="class">
  <name>Bignum</name>
  <superclass>Integer</superclass>
  <implementation>org.jruby.RubyBignum</implementation>

  <instance-methods>
    <method name="~">
      <java>op_invert</java>
    </method>
    <method name="&amp;">
      <arity>1</arity>
      <java>op_and</java>
    </method>
    <method name="&lt;">
      <arity>1</arity>
      <java>op_lt</java>
    </method>
    <method name="&lt;=">
      <arity>1</arity>
      <java>op_le</java>
    </method>
    <method name="&lt;=>">
      <arity>1</arity>
      <java>op_cmp</java>
    </method>
    <method name="&lt;&lt;">
      <arity>1</arity>
      <java>op_lshift</java>
    </method>
    <method name="%">
      <arity>1</arity>
      <java>op_mod</java>
    </method>
    <method name="+">
      <arity>1</arity>
      <java>op_plus</java>
    </method>
    <method name="*">
      <arity>1</arity>
      <java>op_mul</java>
    </method>
    <method name="**">
      <arity>1</arity>
      <java>op_pow</java>
    </method>
    <method name="-">
      <arity>1</arity>
      <java>op_minus</java>
    </method>
    <method-alias name="modulo" original="%"/>
    <method name="/">
      <arity>1</arity>
      <java>op_div</java>
    </method>
    <method name=">">
      <arity>1</arity>
      <java>op_gt</java>
    </method>
    <method name=">=">
      <arity>1</arity>
      <java>op_ge</java>
    </method>
    <method name=">>">
      <arity>1</arity>
      <java>op_rshift</java>
    </method>
    <method name="|">
      <arity>1</arity>
      <java>op_or</java>
    </method>
    <method name="^">
      <arity>1</arity>
      <java>op_xor</java>
    </method>
    <method name="-@">
      <java>op_uminus</java>
    </method>
    <method name="[]">
      <arity>1</arity>
      <java>aref</java>
    </method>
    <method name="==">
      <arity>1</arity>
      <java>op_equal</java>
    </method>
    <method-alias name="===" original="=="/>
    <method-alias name="eql?" original="=="/>
    <method name="coerce">
      <arity>1</arity>
    </method>
    <method name="remainder">
      <arity>1</arity>
    </method>
    <method name="hash"/>
    <method name="size"/>
    <method name="to_f"/>
    <method name="to_i"/>
    <method name="to_s"/>
  </instance-methods>

  <class-methods/>
</module>