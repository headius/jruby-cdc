<module type="class">
  <name>Numeric</name>
  <superclass>Object</superclass>
  <includes>Comparable</includes>
  <index-offset>0x6000</index-offset>

  <implementation>org.jruby.RubyNumeric</implementation>

  <instance-methods>
    <method name="abs"/>
    <method name="clone">
      <java>rbclone</java>
    </method>
    <method name="ceil"/>
    <method name="coerce">
      <arity>1</arity>
    </method>
    <method name="divmod">
      <arity>1</arity>
    </method>
    <method name="eql?">
      <arity>1</arity>
      <java>eql</java>
    </method>
    <method name="===">
      <arity>1</arity>
      <java>equal</java>
    </method>
    <method name="floor"/>
    <method name="integer?">
      <java>int_p</java>
    </method>
    <method name="modulo">
      <arity>1</arity>
    </method>
    <method name="nonzero?">
      <java>nonzero_p</java>
    </method>
    <method name="-@">
      <java>op_uminus</java>
    </method>
    <method name="+@">
      <java>op_uplus</java>
    </method>
    <method name="remainder">
      <arity>1</arity>
    </method>
    <method name="round"/>
    <method name="truncate"/>
    <method name="zero?">
      <java>zero_p</java>
    </method>
  </instance-methods>

  <class-methods/>
</module>
