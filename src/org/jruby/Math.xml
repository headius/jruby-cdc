<module type="module">
  <name>Math</name>
  <implementation>org.jruby.RubyMath</implementation>
  <index-offset>0x6700</index-offset>

  <class-methods>
    <method name="atan2">
      <arity>2</arity>
    </method>
    <method name="cos">
      <arity>1</arity>
    </method>
    <method name="exp">
      <arity>1</arity>
    </method>
    <method name="frexp">
      <arity>1</arity>
    </method>
    <method name="ldexp">
      <arity>2</arity>
    </method>
    <method name="log">
      <arity>1</arity>
    </method>
    <method name="log10">
      <arity>1</arity>
    </method>
    <method name="sin">
      <arity>1</arity>
    </method>
    <method name="sqrt">
      <arity>1</arity>
    </method>
    <method name="tan">
      <arity>1</arity>
    </method>
  </class-methods>
</module>
