#!/usr/bin/env /Users/enebo/workspace/jruby_trunk/bin/jruby -v
#
#   irb.rb - intaractive ruby
#   	$Release Version: 0.7.3 $
#   	$Revision$
#   	$Date$
#   	by Keiju ISHITSUKA(keiju@ishitsuka.com)
#

if defined?(JRUBY_VERSION)
  require 'jruby'
  # useful for code completion
  JRuby.objectspace = true
end

require "irb"

if __FILE__ == $0
  IRB.start(__FILE__)
else
  # check -e option
  if /^-e$/ =~ $0
    IRB.start(__FILE__)
  else
    IRB.setup(__FILE__)
  end
end
